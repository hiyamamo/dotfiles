	"mocha's it": {
		"prefix": "itm",
		"body": [
			"it('$1', () => {",
			"\texpect(${2:subject}).to.eql(${3:expected})",
			"})"
		],
		"description": "Insert mocha's example"
	},
	"mocha's context": {
		"prefix": "ctx",
		"body": [
			"context('$1', () => {",
			"\t$2",
			"})"
		],
		"description": "Insert mocha's context"
	},
}